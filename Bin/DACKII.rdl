  <DataSets>
    <DataSet Name="DataSetGenreRegion">
      <Query>
        <DataSourceName>DataSourceGenreRegion</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Genre">
            <Value>=Parameters!Genre.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DimRegion.region_name, DimGenre.genre_name, FactGameSale.num_sales
FROM     DimGenre INNER JOIN
                  FactGameSale ON DimGenre.id = FactGameSale.genre_id INNER JOIN
                  DimRegion ON FactGameSale.region_id = DimRegion.id
WHERE  (DimRegion.region_name IN (@Region)) AND (DimGenre.genre_name IN (@Genre))</CommandText>
      </Query>
      <Fields>
        <Field Name="region_name">
          <DataField>region_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="genre_name">
          <DataField>genre_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="num_sales">
          <DataField>num_sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Region">
      <Query>
        <DataSourceName>DataSourceGenreRegion</DataSourceName>
        <CommandText>SELECT DISTINCT region_name AS Region
FROM     DimRegion
ORDER BY Region</CommandText>
      </Query>
      <Fields>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
